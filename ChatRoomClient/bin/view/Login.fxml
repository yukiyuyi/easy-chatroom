<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginController">
   <children>
      <GridPane layoutX="92.0" layoutY="83.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="70.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="0.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="账号" />
            <Label text="密码" GridPane.rowIndex="1" />
            <TextField fx:id="unameField" GridPane.columnIndex="1" />
            <PasswordField fx:id="upassField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Label layoutX="155.0" layoutY="51.0" text="登陆" AnchorPane.leftAnchor="140.0" AnchorPane.rightAnchor="135.0" AnchorPane.topAnchor="40.0" />
      <Button layoutX="62.0" layoutY="152.0" mnemonicParsing="false" onAction="#register" text="注册" AnchorPane.leftAnchor="62.0" />
      <Button layoutX="133.0" layoutY="152.0" mnemonicParsing="false" onAction="#handleOk" text="登录" AnchorPane.rightAnchor="127.0" />
      <Button layoutX="203.0" layoutY="152.0" mnemonicParsing="false" onAction="#handleCancel" text="取消" />
   </children>
</AnchorPane>
