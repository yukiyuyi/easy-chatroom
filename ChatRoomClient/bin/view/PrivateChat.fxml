<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.PrivateChatController">
   <children>
      <Label layoutX="138.0" layoutY="31.0" text="私聊" />
      <GridPane layoutX="50.0" layoutY="79.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="43.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="169.0" minWidth="10.0" prefWidth="157.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="53.0" minHeight="10.0" prefHeight="46.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="内容" />
            <TextArea fx:id="privateMessage" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="81.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleOkButton" text="发送" />
      <Button layoutX="177.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleCancelButton" text="取消" />
   </children>
</AnchorPane>
