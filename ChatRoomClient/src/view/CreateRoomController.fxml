<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CreateRoomController">
   <children>
      <Label layoutX="120.0" layoutY="30.0" text="创建聊天室" />
      <GridPane layoutX="50.0" layoutY="70.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="58.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="142.0" minWidth="10.0" prefWidth="142.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="聊天室名" />
            <TextField fx:id="chatroomId" GridPane.columnIndex="1" />
            <Label text="端口号" GridPane.rowIndex="1" />
            <TextField fx:id="portField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="100.0" layoutY="147.0" mnemonicParsing="false" onAction="#handleOkButton" text="提交" />
      <Button layoutX="174.0" layoutY="147.0" mnemonicParsing="false" onAction="#handleCancelButton" text="取消" />
   </children>
</AnchorPane>
